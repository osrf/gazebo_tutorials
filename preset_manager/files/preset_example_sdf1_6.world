          </collision>
          <visual name="visual">
            <geometry>
              <box>
                <size>0.010000000000000002 0.010000000000000002 0.1</size>
              </box>
            </geometry>
          </visual>
        </link>
        <joint name="upper_joint" type="revolute">
          <pose>0 0 0.05  0 0 0</pose>
